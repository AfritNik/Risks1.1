<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DropShadowEffect x:Key="dark-90" Direction="-90" ShadowDepth="1" Opacity="0.3" BlurRadius="3"/>

    
    
    <ControlTemplate x:Key="defaultSplitButton" TargetType="{x:Type ToggleButton}" >

        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
            <Border BorderBrush="#7F0F235C" BorderThickness="1,1,0,0" CornerRadius="30" Background="#3312265E"/>
            <Border BorderBrush="#7FFFFFFF" BorderThickness="0,0,1,1" CornerRadius="30"/>
            <Border x:Name="circleBorder" BorderBrush="#FF133E8A" BorderThickness="1" CornerRadius="30" Margin="4.167,3.499,4.167,4.835" Effect="{StaticResource ResourceKey=dark-90}">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFC0D0EC" Offset="0"/>
                        <GradientStop Color="#FFA3B8E0" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>

            </Border>
            <Path x:Name="icon" Stretch="Fill" Fill="#FF182B4B" RenderTransformOrigin="0.5,0.4" Data="F1 M 1103.87,11.6153C 1102.91,11.6153 1101.95,11.2715 1101.19,10.5841L 1088.77,-0.63472C 1087.13,-2.11512 1087,-4.64512 1088.48,-6.28445C 1089.96,-7.92245 1092.49,-8.05259 1094.13,-6.57218L 1103.87,2.22208L 1113.59,-6.58126C 1115.23,-8.06565 1117.76,-7.93805 1119.24,-6.30139C 1120.72,-4.66325 1120.6,-2.13473 1118.96,-0.65152L 1106.56,10.5802C 1105.79,11.2703 1104.83,11.6153 1103.87,11.6153 Z " Margin="16,18,16,18"/>
        </Grid>

        <ControlTemplate.Triggers>

            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="circleBorder" Property="Background">
                    <Setter.Value>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFD3DEF3" Offset="1"/>
                            <GradientStop Color="#FFA4B9E2"/>
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter TargetName="icon" Property="RenderTransform">
                    <Setter.Value>
                        <ScaleTransform ScaleY="-1"/>
                    </Setter.Value>
                </Setter>
            </Trigger>

            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="circleBorder" Property="Background">
                    <Setter.Value>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="White"/>
                            <GradientStop Color="#FFD1DEF9" Offset="0.854"/>
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter TargetName="circleBorder" Property="BorderBrush" Value="#FF09285E" />
            </Trigger>

        </ControlTemplate.Triggers>

    </ControlTemplate>




    <Color x:Key="NormalStateSubmitBackground0" >#FF51E02B</Color>
    <Color x:Key="NormalStateSubmitBackground1" >#FF3DB81D</Color>

    <Color x:Key="PressedStateSubmitBackground0" >#FF31C00B</Color>
    <Color x:Key="PressedStateSubmitBackground1" >#FF45C823</Color>

    <Color x:Key="MouseOverStateSubmitBackground0" >#FF68FB41</Color>
    <Color x:Key="MouseOverStateSubmitBackground1" >#FF4ACC28</Color>

    <SolidColorBrush x:Key="IconSubmitNormalState" Color="White" />
    <Color x:Key="IconSubmitOtherState" >Black</Color>

    <ControlTemplate x:Key="submitButton" TargetType="{x:Type Button}">
        <Grid>
            <Border BorderBrush="#7F0F235C" BorderThickness="1,1,0,0" CornerRadius="15" Background="#3312265E"/>
            <Border BorderBrush="#7FFFFFFF" BorderThickness="0,0,1,1" CornerRadius="15"/>
            <Border x:Name="MainBorder" BorderBrush="#FF213F74" BorderThickness="1" CornerRadius="12" Margin="4" Effect="{DynamicResource dark-90}">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="{StaticResource ResourceKey=NormalStateSubmitBackground0}" Offset="0"/>
                        <GradientStop Color="{StaticResource ResourceKey=NormalStateSubmitBackground1}" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>

                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,6">
                    <Path Stretch="Fill" x:Name="icon" Data="M 1095.86,-7.14016C 1095.86,-7.75736 1096.35,-8.31203 1097.1,-8.54895C 1097.84,-8.78603 1098.7,-8.65322 1099.28,-8.21828L 1111.28,0.922386C 1111.67,1.22184 1111.86,1.60984 1111.86,2.00051C 1111.86,2.38851 1111.67,2.77917 1111.28,3.07597L 1099.28,12.2218C 1098.7,12.6568 1097.84,12.7896 1097.1,12.5525C 1096.35,12.3156 1095.86,11.7609 1095.86,11.1437L 1095.86,-7.14016 Z " 
                          Margin="12,0,6,0" Fill="{StaticResource IconSubmitNormalState}" HorizontalAlignment="Left" Width="12" Height="12">
                        <Path.Effect>
                            <DropShadowEffect Direction="90" BlurRadius="2" ShadowDepth="1" Color="#FF213F74" Opacity="0.4"/>
                        </Path.Effect>
                    </Path>
                    <TextBlock x:Name="txt" Margin="0,0,12,2" TextWrapping="Wrap" Text="{TemplateBinding Content}" FontSize="14" FontFamily="Microsoft Sans Serif" Foreground="{StaticResource IconSubmitNormalState}"/>
                </StackPanel>

            </Border>
            <Border  BorderBrush="#33FFFFFF" BorderThickness="1" CornerRadius="11" Margin="5" Effect="{DynamicResource dark-90}"/>

            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="0:0:0.0" />
                        <VisualTransition GeneratedDuration="0:0:0.0" To="Pressed" />
                        <VisualTransition GeneratedDuration="0:0:0.0" From="Pressed" />
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation  Storyboard.TargetName="txt" Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=IconSubmitOtherState}" Duration="0:0:0.1"/>
                            <ColorAnimation  Storyboard.TargetName="icon" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=IconSubmitOtherState}" Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="MainBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[0].(GradientStop.Color)" To="{StaticResource ResourceKey=MouseOverStateSubmitBackground0}"  Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="MainBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[1].(GradientStop.Color)" To="{StaticResource ResourceKey=MouseOverStateSubmitBackground1}"  Duration="0:0:0.1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation  Storyboard.TargetName="txt" Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=IconSubmitOtherState}" Duration="0:0:0.1"/>
                            <ColorAnimation  Storyboard.TargetName="icon" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=IconSubmitOtherState}" Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="MainBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[0].(GradientStop.Color)" To="{StaticResource ResourceKey=PressedStateSubmitBackground0}"  Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="MainBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[1].(GradientStop.Color)" To="{StaticResource ResourceKey=PressedStateSubmitBackground1}"  Duration="0:0:0.1"/>

                        </Storyboard>
                    </VisualState>
                    <!--<VisualState x:Name="Disabled">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=DisabledStateBorder}" Duration="0:0:0.1"/>
      
                        </Storyboard>
                    </VisualState>-->
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>

        </Grid>
    </ControlTemplate>



    <ControlTemplate x:Key="parametersToggleButton" TargetType="{x:Type ToggleButton}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            
            <Border x:Name="ToggleBorder" Grid.Column="0" BorderBrush="#FF133E8A" BorderThickness="1" CornerRadius="3" Grid.ColumnSpan="2" Margin="0,3,3,0">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="{StaticResource NormalStateBackground0}" Offset="0"/>
                        <GradientStop Color="{StaticResource NormalStateBackground1}" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
         
            <TextBlock Text="{TemplateBinding Content}" VerticalAlignment="Center" FontFamily="Microsoft Sans Serif" FontSize="13" Foreground="Black" Margin="8,3,8,0" />

            <Border x:Name="borderOrange" Background="#DDFFA30A" CornerRadius="6" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,0,0,0" BorderThickness="0" >
                <TextBlock Grid.Column="1" Text="{TemplateBinding Tag}" VerticalAlignment="Center" FontFamily="Microsoft Sans Serif" FontSize="9" Foreground="Black" Margin="4,2" />
            </Border>

            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CheckStates">
                    <VisualState x:Name="Unchecked" />
                    <VisualState x:Name="Checked" >
                        <Storyboard >
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=MouseOverStateBorder}"  Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[0].(GradientStop.Color)" To="{StaticResource ResourceKey=MouseOverStateBackground0}"  Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[1].(GradientStop.Color)" To="{StaticResource ResourceKey=MouseOverStateBackground1}"  Duration="0:0:0.1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>

                <VisualStateGroup x:Name="CommonStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="0:0:0.0" />
                        <VisualTransition GeneratedDuration="0:0:0.0" To="Pressed" />
                        <VisualTransition GeneratedDuration="0:0:0.0" From="Pressed" />
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>

                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=PressedStateBorder}"  Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[0].(GradientStop.Color)" To="{StaticResource ResourceKey=PressedStateBackground0}"  Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[1].(GradientStop.Color)" To="{StaticResource ResourceKey=PressedStateBackground1}"  Duration="0:0:0.1"/>


                        </Storyboard>
                    </VisualState>
                    <!--<VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=PressedStateBorder}"  Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[0].(GradientStop.Color)" To="{StaticResource ResourceKey=PressedStateBackground0}"  Duration="0:0:0.1"/>
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.Background).(LinearGradientBrush.GradientStops)[1].(GradientStop.Color)" To="{StaticResource ResourceKey=PressedStateBackground1}"  Duration="0:0:0.1"/>
                            <ColorAnimation  Storyboard.TargetName="Arrow" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=IconOtherState}" Duration="0:0:0.1"/>
                        </Storyboard>
                    </VisualState>-->
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetName="ToggleBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" To="{StaticResource ResourceKey=DisabledStateBorder}" Duration="0:0:0.1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>


        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="Tag" Value="">
                <Setter TargetName="borderOrange" Property="Visibility" Value="Hidden" />
            </Trigger>
        </ControlTemplate.Triggers>
        
    </ControlTemplate>

