    <ControlTemplate x:Key="transparentButton" TargetType="{x:Type Button}">

        <Border Grid.ColumnSpan="2" Background="Transparent" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" x:Name="MainBorder" >
            <TextBlock Grid.Column="1" x:Name="txt" Foreground="#FF2D2D2D" Text="{TemplateBinding Content}" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="7,0,7,0" FontFamily="Microsoft Sans Serif" FontSize="13" />
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="MainBorder" Property="Background" Value="#FF0495FF" />
                <Setter TargetName="txt" Property="Foreground" Value="White" />
            </Trigger>
        </ControlTemplate.Triggers>

    </ControlTemplate>
